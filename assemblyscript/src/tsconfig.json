{
  "extends": "../std/assembly.json",
  "compilerOptions": {
    "target": "esnext",
    "outDir": "../build",
    "allowJs": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "useDefineForClassFields": false,
    "strict": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "esModuleInterop": true,
    "noEmit": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["../**/node_modules/", "../tests/**", "../lib/**"]
}
