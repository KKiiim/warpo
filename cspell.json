{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "language": "en",
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "cspell/project-words.txt",
      "addWords": true
    }
  ],
  "dictionaries": ["project-words", "typescript", "node", "powershell", "cpp", "html", "bash"],
  "ignorePaths": [
    "build*",
    "*.svg",
    "*.wast",
    "*.wat",
    "*.pftrace",
    "passes/DoInlining.cpp",
    "passes/DoInlining.hpp",
    "third_party/**",
    "assemblyscript/build/**",
    "assemblyscript/cli/**",
    "assemblyscript/dist/**",
    "assemblyscript/lib/**",
    "assemblyscript/media/**",
    "assemblyscript/node_modules/**",
    "assemblyscript/scripts/**",
    "assemblyscript/util/**",
    "assemblyscript/NOTICE",
    "assemblyscript/package.json",
    "assemblyscript/src/glue/binaryen.d.ts",
    "assemblyscript/src/glue/binaryen.js",
    "assemblyscript/src/module.ts",
    "assemblyscript/std",
    "tests/bootstrap/tmp/**",
    "tests/frontend/compiler/**",
    "tests/DebugSymbol/*.txt",
    "warpo_extension/std/wasi_snapshot_preview1/bindings/wasi_snapshot_preview1.ts"
  ]
}
